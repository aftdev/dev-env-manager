{
  "name": "@aftdev/dev-env-manager",
  "version": "0.1.0",
  "description": "Command line utilities to manage your projects development environment",
  "type": "module",
  "engines": {
    "node": ">=16.0"
  },
  "bin": {
    "dev": "./bin/index.js"
  },
  "author": "Christophe G",
  "license": "MIT",
  "dependencies": {
    "awilix": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "findup-sync": "^5.0.0",
    "inquirer": "^8.2.0",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sinon": "^12.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --cache --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "format": "pretty-quick",
    "test": "mocha",
    "test-all": "mocha --reporter=dot --recursive './tests/**/*.test.js'",
    "coverage": "c8 --reporter=html --reporter=text --reporter=html yarn test-all",
    "publish": "npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
