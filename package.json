{
  "name": "@aftdev/dev-env-manager",
  "version": "1.0.3",
  "description": "Command line utilities to manage your projects development environment",
  "type": "module",
  "keywords": [
    "development",
    "environment manager",
    "docker",
    "package manager"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "bin": {
    "dev": "./bin/index.js"
  },
  "author": "Christophe G",
  "repository": {
    "type": "git",
    "url": "https://github.com/aftdev/dev-env-manager"
  },
  "license": "MIT",
  "files": [
    "bin",
    "docs",
    "src",
    "CHANGELOG.md",
    "README.md"
  ],
  "dependencies": {
    "awilix": "^8.0.0",
    "chalk": "^5.2.0",
    "chalk-template": "^1.0.0",
    "commander": "^10.0.0",
    "enquirer": "^2.3.6",
    "extract-stack": "^3.0.0",
    "findup-sync": "^5.0.0",
    "nconf": "^0.12.0",
    "nconf-yaml": "^1.0.2",
    "path": "^0.12.7",
    "puka": "^1.0.1",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.3",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "sinon": "^15.0.1",
    "strip-ansi": "^7.0.1"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#services/*": "./src/services/*.js",
    "#tests/*": "./tests/*.js"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --cache --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write ./**/*.{js,mjs,cjs,ts,md,json,yaml,yml}",
    "test": "mocha",
    "test-all": "mocha --reporter=dot --recursive './tests/**/*.test.js'",
    "coverage": "c8 --reporter=html --reporter=text --reporter=html yarn test-all",
    "ci:coverage": "c8 --check-coverage --reporter=none yarn test-all",
    "release": "changeset publish"
  },
  "packageManager": "yarn@3.5.0"
}
