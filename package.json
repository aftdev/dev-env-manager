{
  "name": "@aftdev/dev-env-manager",
  "version": "0.5.0",
  "description": "Command line utilities to manage your projects development environment",
  "type": "module",
  "keywords": [
    "development",
    "environment manager",
    "docker",
    "package manager"
  ],
  "engines": {
    "node": ">=16.0"
  },
  "bin": {
    "dev": "./bin/index.js"
  },
  "author": "Christophe G",
  "repository": {
    "type": "git",
    "url": "https://github.com/aftdev/dev-env-manager"
  },
  "license": "MIT",
  "files": [
    "bin",
    "docs",
    "src",
    "CHANGELOG.md",
    "README.md"
  ],
  "dependencies": {
    "awilix": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "extract-stack": "^3.0.0",
    "findup-sync": "^5.0.0",
    "inquirer": "^8.2.0",
    "nconf": "^0.11.3",
    "nconf-yaml": "^1.0.2",
    "path": "^0.12.7",
    "puka": "^1.0.1",
    "url": "^0.11.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "sinon": "^12.0.1",
    "strip-ansi": "^7.0.1"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#services/*": "./src/services/*.js",
    "#tests/*": "./tests/*.js"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --cache --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write ./**/*.{js,mjs,cjs,ts,md,json,yaml,yml}",
    "test": "mocha",
    "test-all": "mocha --reporter=dot --recursive './tests/**/*.test.js'",
    "coverage": "c8 --reporter=html --reporter=text --reporter=html yarn test-all",
    "ci:coverage": "c8 --check-coverage --reporter=none yarn test-all",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
